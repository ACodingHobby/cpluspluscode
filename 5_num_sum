#include <iostream>
using namespace std;

int main() {
    const int SIZE = 5;
    int arr[SIZE];
    int sum = 0;
    
    cout << "Enter 5 integers:" << endl;
    for (int i = 0; i < SIZE; i++) {
        cout << "Enter integer number " << i + 1 << ": ";
        cin >> arr[i];
    }
        
    for (int i = 0; i < SIZE; i++) {
        sum += arr[i];
    }
    cout << sum;
}
